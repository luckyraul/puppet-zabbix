UserParameter=nginx.version,  nginx -v 2>&1 | cut -f2 -d'/'
UserParameter=nginx.active,   wget -O- -q http://localhost/nginx_status | head -1 | cut -f3 -d' '
UserParameter=nginx.accepts,  wget -O- -q http://localhost/nginx_status | grep -Ev '[a-zA-Z]' | cut -f2 -d' '
UserParameter=nginx.handled,  wget -O- -q http://localhost/nginx_status | grep -Ev '[a-zA-Z]' | cut -f3 -d' '
UserParameter=nginx.requests, wget -O- -q http://localhost/nginx_status | grep -Ev '[a-zA-Z]' | cut -f4 -d' '
UserParameter=nginx.reading,  wget -O- -q http://localhost/nginx_status | tail -1 | cut -f2 -d' '
UserParameter=nginx.writing,  wget -O- -q http://localhost/nginx_status | tail -1 | cut -f4 -d' '
UserParameter=nginx.waiting,  wget -O- -q http://localhost/nginx_status | tail -1 | cut -f6 -d' '
